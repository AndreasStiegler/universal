{
    "typeName": "deviceReporter.development",
    "options": {
        "components": {
            "server": {
                "options": {
                    "logging": true,
                    "components": {
                        "deviceReporter": {
                            "options": {
                                "installedSolutionsUrl": "file://%root/../../../testData/deviceReporter/installedSolutions.json"
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [{
        "demandingName": "gpii.urlExpander",
        "context": "gpii.deviceReporter",
        "demandSpec": {
            "options": {
                "vars": {
                    "port": "{gpii.server}.options.port",
                    "root": "{gpii.deviceReporter}.options.root"
                }
            }
        }
    }]
}