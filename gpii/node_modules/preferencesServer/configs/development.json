{
    "typeName": "preferencesServer.development",
    "options": {
        "components": {
            "server": {
                "options": {
                    "logging": true,
                    "components": {
                        "preferencesServer": {
                            "options": {
                                "userSourceUrl": "file://%root/../../../testData/preferences/%token.json"
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [{
        "demandingName": "gpii.urlExpander",
        "context": "gpii.preferencesServer",
        "demandSpec": {
            "options": {
                "vars": {
                    "port": "{gpii.server}.options.port",
                    "root": "{gpii.preferencesServer}.options.root"
                }
            }
        }
    }, {
        "demandingName": "rawUserSource",
        "context": "gpii.preferencesServer",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL"
        }
    }, {
        "demandingName": "userSource",
        "context": "gpii.preferencesServer",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawUserSource}"
                }
            }
        }
    }]
}