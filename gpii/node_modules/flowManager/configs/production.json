{
    "typeName": "flowManager.development",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "components": {
            "server": {
                "type": "gpii.server",
                "options": {
                    "logging": false,
                    "components": {
                        "flowManager": {
                            "type": "gpii.flowManager",
                            "options": {
                                "urls": {
                                    "preferences": "http://localhost:8080/user/%token",
                                    "deviceReporter": "http://localhost:8081/device",
                                    "solutionsReporter": "http://localhost:8081/solutions",
                                    "matchMaker": "http://localhost:8081/match"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}