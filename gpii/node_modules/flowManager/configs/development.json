{
    "typeName": "flowManager.development",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "distributeOptions": [{
            "source": "{that}.options.flowManagerDevGrades",
            "target": "{that flowManager}.options.gradeNames"
        }, {
            "source": "{that}.options.devUrls",
            "target": "{that flowManager}.options.urls"
        }, {
            "source": "{that}.options.matchMakers",
            "target": "{that flowManager}.options.matchMakers"
        }, {
            "source": "{that}.options.matchMakerDispatcherDisabled",
            "target": "{that flowManager}.options.matchMakerDispatcherDisabled"
        }],
        "flowManagerDevGrades": ["kettle.urlExpander.distributeDevVariables"],
        "devUrls": {
            "preferences": "http://localhost:%port/preferences/%userToken",
            "deviceReporter": "http://localhost:%port/device",
            "solutionsRegistry": "file://%root/../../../testData/solutions/%os.json"
        },
        "matchMakers": {
            "flat": {
                "url": "http://localhost:8081"
            },
            "Statistical1": {
                "url": "http://smm1.gpii.eu:8076"
            },
            "Statistical2": {
                "url": "http://smm2.gpii.eu:8075"
            },
            "Statistical3": {
                "url": "http://smm3.gpii.eu:8074"
            },
            "Statistical4": {
                "url": "http://smm4.gpii.eu:8073"
            }
        },
        "matchMakerDispatcherDisabled": true
    },
    "includes": [
        "./base.json",
        "./io.json"
    ]
}