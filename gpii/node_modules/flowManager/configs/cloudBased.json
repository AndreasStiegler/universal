{
    "typeName": "flowManager.cloudBased",
    "options": {
        "components": {
            "server": {
                "options": {
                    "components": {
                        "flowManager": {
                            "type": "gpii.flowManager.cloudBased",
                            "options": {
                                "typeName": "flowManager.cloudBased",
                                "urls": {
                                    "deviceReporter": ""
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [
        {
            "demandingName": "kettle.requests.request.handler",
            "contextNames": ["userLogin", "gpii.flowManager.cloudBased"],
            "demandSpec": {
                "options": {
                    "invokers": {
                        "handle": {
                            "funcName": "kettle.requests.request.handler.userLogin.cloudBased",
                            "args": ["{request}.req.params.token", "{that}", "{request}.req.body.device"]
                        }
                    },
                    "listeners": {
                        "onUserListener.local": "fluid.identity"
                    },
                    "gradeNames": ["kettle.requests.request.handler.userLoginHandler.cloudBased"]
                }
            }
        },
        {
            "demandingName": "rawDeviceReporterDataSource",
            "contextNames": ["gpii.flowManager", "gpii.flowManager.cloudBased"],
            "demandSpec": {
                "funcName": "fluid.emptySubcomponent"
            }
        },
        {
            "demandingName": "deviceReporterDataSource",
            "contextNames": ["gpii.flowManager", "gpii.flowManager.cloudBased"],
            "demandSpec": {
                "funcName": "fluid.emptySubcomponent"
            }
        }
    ],
    "includes": [
        "./production.json"
    ]
}