                    "ZoomIncrement": {
                    	"value": 100,
                    	"type": "REG_DWORD"
                    },
                    "Invert": {
                    	"value": 0,
                    	"type": "REG_DWORD"
                    },
                    "FollowMouse": {
                    	"value": 1,
                    	"type": "REG_DWORD"
                    },
                    "FollowFocus": {
                    	"value": 1,
                    	"type": "REG_DWORD"
                    },
                    "FollowCaret": {
                    	"value": 1,
                    	"type": "REG_DWORD"
                    },
                    "MagnificationModel": {
                    	"value": 1,
                    	"type": "REG_DWORD"
                    },
                }
            }]
        }
    },
