{
    "display": {
        "screenReader": {
            "applications": [{
                 "name": "GNOME Shell Screen Reader",
                "id": "org.gnome.desktop.a11y.keyboard",
                "priority": 100
            }, {
                "name": "NVDA Screen Reader",
                "id": "nvda.screenReader",
                "priority": 100
            }] 
        },
        "applications": {
            "name": "UI Options",
            "id": "fluid.uiOptions",
            "parameters": {
                "lineSpacing": "1",
                "links": false,
                "inputsLarger": false,
                "captions": false,
                "transcripts": false,
                "volume": "50"
            }
        },
        "screenEnhancement": {
            "fontSize": 12,
            "foregroundColor": "white",
            "backgroundColor": "black",
            "fontFace": {
                "fontName": [
                    "Comic Sans"
                ],
                "genericFontFace": "sans serif"
            }
        }
    },
    "content": {
        "adaptationPreference": [
            {
                "adaptationType": "caption",
                "language": "en"
            },
            {}
        ]
    },
    "control": {
        "keyboardEnhancement": {
            "applications": [{                
                "name": "GNOME Shell Keyboard Settings",
                "id": "org.gnome.desktop.a11y.keyboard",
                "priority": 100,
                "parameters": {
                    "feature-state-change-beep": true
                }
            }]
        },
        "structuralNavigation": {
            "tableOfContents": false
        }
    }
}