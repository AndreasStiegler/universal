        "name": "Windows Built-in Font Sizing",
        "id": "com.microsoft.windows.font-size",
        "version": "7",
        "contexts": {
            "OS": [
                {
                    "id": "com.microsoft.windows",
                    "version": ">=7.0"
                }
            ]
        },
        "settingsHandlers": [
            {
                "type": "gpii.settings.SPIHandler",
                "options": {},
                "capabilities": []
            }
        ],
        "lifecycleHandlers": {
            "start": [ "setSettings" ],
            "stop": [ "restoreSettings" ]
        }
    }, {
        "name": "Windows Built-in Screen Magnifier",
        "id": "com.microsoft.windows.magnifier",
        "version": "7",
        "contexts": {
            "OS": [
                {
                    "id": "com.microsoft.windows",
                    "version": ">=7.0"
                }
            ]
        },
        "settingsHandlers": [
            {
                "type": "gpii.settings.windowsRegistryHandler",
                "options": {
                    "hKey": "HKEY_CURRENT_USER",
                    "path": "Software\\Microsoft\\ScreenMagnifier"
                },
                "capabilities": [
                    "Magnification",
                    "ZoomIncrement",
                    "Invert",
                    "FollowMouse",
                    "FoloowFocus",
                    "FollowCaret",
                    "MagnificationMode"
                ]
            }
        ],
        "lifecycleHandler": {
            "start": [ 
                "setSettings", 
                {
                    "type": "gpii.launch.exec",
                    "command": "${{environment}.SystemRoot}\\System32\\Magnify.exe"
                }
            ],
            "stop": [ {
                    "type": "gpii.launch.spawn",
                    "command": "taskkill.exe /im Magnify.exe"
                },
                "restoreSettings"
            ]
        }
    }, {
        "name": "Windows Built-in OnScreen Keyboard",
        "id": "com.microsoft.windows.onscreenKeyboard",
        "version": "7",
        "contexts": {
