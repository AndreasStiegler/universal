            "OS": [
                {
                    "id": "com.microsoft.windows",
                    "version": ">=7.0"
                }
            ]
        },
        "settingsHandlers": [
            {
                "type": "gpii.settings.SPIHandler",
                "options": {},
                "capabilities": [
                    "control.keyboardEnhancement.stickyKeys",
                    "control.keyboardEnhancement.repeatKeys",
                    "control.keyboardEnhancement.slowkeys",
                    "control.keyboardEnhancement.debounceKeys"
                ]
            }
        ],
        "lifecycleHandlers": {
            "start": [ "setSettings" ],
            "stop": [ "restoreSettings" ]
        }
    }, {
        "name": "NVDA Screen Reader",
        "id": "nvda.screenReader",
        "contexts": {
            "OS": [
                {
                    "id": "com.microsoft.windows",
                    "version": ">=7.0"
                }
            ]
        },
        "settingsHandlers": [],
        "lifecycleHandler": {
            "start": [ 
                {
                    "type": "gpii.launch.exec",
                    "command": "C:\\Program Files (x86)\\NVDA\\nvda.exe"
                }
            ],
            "stop": [ {
                    "type": "gpii.launch.spawn",
                    "command": "taskkill.exe /f /im nvda_service.exe"
                },{
                    "type": "gpii.launch.spawn",
                    "command": "taskkill.exe /im nvda.exe"
                }
            ]
        }
    }, {
        "name": "GNOME Nautilus Desktop Font",
        "id": "org.gnome.nautilus.desktop",
        "version": "3.2.1",
        "contexts": {
            "OS": {
                "id": "linux",
                "version": ">=2.6.26"
            }
        },
        "settingsHandlers": [
            {
                "type": "gpii.gsettings.set",
