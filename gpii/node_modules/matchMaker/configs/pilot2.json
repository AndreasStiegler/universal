{
    "typeName": "matchMakerProxy",
    "options": {
        "gradeNames": [
            "autoInit",
            "fluid.littleComponent"
        ],
        "components": {
            "matchMaker": {
                "type": "gpii.matchMaker",
                "options": {
                    "gradeNames": [
                        "gpii.matchMaker.dev"
                    ],
                    "components": {
                        "callbackWrapper": "{flatMatchMakerServer}.callbackWrapper"
                    },
                    "solutionsReporterUrl": "file://%root/../../../testData/solutions/%os.json",
                    "ontologyServerUrl": "file://%root/../../../testData/ontologies/ontologies.json"
                }
            },
            "server": {
                "type": "kettle.server",
                "options": {
                    "components": {
                        "matchMakerProxy": {
                            "type": "gpii.matchMakerProxy",
                            "options": {
                                "matchMakerPathMap": {
                                    "flat": "http://localhost:8079",
                                    "ruleBased": "http://rbmm.gpii.net",
                                    "statistical": "http://stmm.gpii.net"
                                }
                            }
                        }
                    }
                }
            },
            "flatMatchMakerServer": {
                "type": "kettle.server",
                "options": {
                    "port": 8079,
                    "components": {
                        "matchMaker": "{gpii.matchMaker}"
                    }
                }
            }
        }
    },
    "includes": [
        "./base.json"
    ],
    "modules": [
        "ontologyServer"
    ]
}