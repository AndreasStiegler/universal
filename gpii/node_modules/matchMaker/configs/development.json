{
    "typeName": "matchMaker.development",
    "options": {
        "components": {
            "server": {
                "options": {
                    "logging": true,
                    "components": {
                        "matchMaker": {
                            "options": {
                                "solutionsReporterUrl": "file://%root/../../../testData/solutions/%os.json",
                                "ontologyServerUrl": "file://%root/../../../testData/ontologies/ontologies.json"
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [{
        "demandingName": "gpii.urlExpander",
        "context": "gpii.matchMaker",
        "demandSpec": {
            "options": {
                "vars": {
                    "port": "{gpii.server}.options.port",
                    "root": "{gpii.matchMaker}.options.root"
                }
            }
        }
    }, {
        "demandingName": "rawOntologyServer",
        "context": "gpii.matchMaker",
        "demandSpec": {
            "funcName": "gpii.ontologyServer.development"
        }
    }]
}