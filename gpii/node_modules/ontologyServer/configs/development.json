{
    "typeName": "ontologyServer.development",
    "options": {
        "components": {
            "server": {
                "options": {
                    "logging": true,
                    "components": {
                        "ontologyServer": {
                            "options": {
                                "ontologySourceUrl": "file://%root/../../../testData/ontologies/%name.json"
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [{
        "demandingName": "gpii.urlExpander",
        "context": "gpii.ontologyServer",
        "demandSpec": {
            "options": {
                "vars": {
                    "port": "{gpii.server}.options.port",
                    "root": "{gpii.ontologyServer}.options.root"
                }
            }
        }
    }, {
        "demandingName": "rawOntologySource",
        "context": "gpii.ontologyServer",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL"
        }
    }]
}