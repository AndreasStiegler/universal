{
    "typeName": "flowManager.preferencesServer.solutionsRegistry.deviceReporter.matchMaker.ontologyServer.production",
    "options": {
        "components": {
            "server": {
                "options": {
                    "logging": false,
                    "components": {
                        "deviceReporter": {
                            "type": "gpii.deviceReporter",
                            "options": {
                                "installedSolutionsUrl": "file://%root/../../../testData/deviceReporter/installedSolutions.json"
                            }
                        },
                        "preferencesServer": {
                            "type": "gpii.preferencesServer",
                            "options": {
                                "userSourceUrl": "http://localhost:5984/user/%token"
                            }
                        },
                        "matchMaker": {
                            "type": "gpii.matchMaker",
                            "options": {
                                "solutionsReporterUrl": "http://localhost:%port/solution?os=%os",
                                "ontologyServerUrl": "http://localhost:%port/ontology/convert/default"
                            }
                        },
                        "solutionsRegistry": {
                            "type": "gpii.solutionsRegistry",
                            "options": {
                                "solutionRegistryUrl": "http://localhost:5984/gpii-sol-registry/_design/gpii-solutions/_rewrite/solutions/%id",
                                "solutionQueryUrl": "http://localhost:5984/gpii-sol-registry/_design/gpii-solutions/_rewrite/solutions?os=%os"
                            }
                        },
                        "ontologyServer": {
                            "type": "gpii.ontologyServer",
                            "options": {
                                "ontologySourceUrl": "http://localhost:5984/ontologies/%name"
                            }
                        },
                        "flowManager": {
                            "type": "gpii.flowManager",
                            "options": {
                                "urls": {
                                    "preferences": "http://localhost:%port/user/%token",
                                    "deviceReporter": "http://localhost:%port/device",
                                    "matchMaker": "http://localhost:%port/match"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [{
        "demandingName": "rawOntologyServer",
        "context": "gpii.matchMaker",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL",
            "options": {
                "url": "{gpii.matchMaker}.options.ontologyServerUrl",
                "writable": true
            }
        }
    }, {
        "demandingName": "rawOntologySource",
        "context": "gpii.ontologyServer",
        "demandSpec": {
            "funcName": "gpii.dataSource.CouchDB"
        }
    }, {
        "demandingName": "rawUserSource",
        "context": "gpii.preferencesServer",
        "demandSpec": {
            "funcName": "gpii.dataSource.CouchDB"
        }
    }, {
        "demandingName": "rawSolutionSource",
        "context": "gpii.solutionsRegistry",
        "demandSpec": {
            "funcName": "gpii.dataSource.CouchDB"
        }
    }, {
        "demandingName": "rawSolutionQuerySource",
        "context": "gpii.solutionsRegistry",
        "demandSpec": {
            "funcName": "gpii.dataSource.CouchDB"
        }
    }]
}