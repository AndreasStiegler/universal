{
    "typeName": "preferencesServer.solutionsRegistry.development",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "components": {
            "server": {
                "type": "gpii.server",
                "options": {
                    "logging": true,
                    "port": 8081,
                    "components": {
                        "preferencesServer": {
                            "type": "gpii.preferencesServer",
                            "options": {
                                "userSourceUrl": "file://%root/../../../testData/preferences/%token.json"
                            }
                        },
                        "solutionsRegistry": {
                            "type": "gpii.solutionsRegistry",
                            "options": {
                                "solutionRegistryUrl": "file://%root/../../../testData/solutions/%id.json",
                                "solutionQueryUrl": "file://%root/../../../testData/solutions/%os.json"
                            }
                        }
                    }
                }
            }
        }
    },
    "demands": [{
        "demandingName": "gpii.urlExpander",
        "context": "preferencesServer.solutionsRegistry.development",
        "demandSpec": {
            "funcName": "gpii.urlExpander.development"
        }
    }, {
        "demandingName": "rawUserSource",
        "context": "gpii.preferencesServer",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL"
        }
    }, {
        "demandingName": "rawSolutionSource",
        "context": "gpii.solutionsRegistry",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL"
        }
    }, {
        "demandingName": "rawSolutionQuerySource",
        "context": "gpii.solutionsRegistry",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL"
        }
    }]
}