{
    "typeName": "gpiiComponent",
    "options": {
        "gradeNames": ["autoInit", "fluid.littleComponent"],
        "components": {
            "server": {
                "type": "gpii.server",
                "options": {
                    "logging": true,
                    "port": 8081
                }
            }
        }
    },
    "demands": [{
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "deviceGet",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.deviceGet",
                        "args": ["{requestProxy}", "{deviceReporter}.installedSolutionsDataSource"]
                    }
                }
            }
        }
    }, {
        "demandingName": "installedSolutionsDataSource",
        "contextNames": "gpii.deviceReporter",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawInstalledSolutionsDataSource}"
                }
            }
        }
    }, {
        "demandingName": "rawInstalledSolutionsDataSource",
        "contextNames": "gpii.deviceReporter",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL",
            "options": {
                "url": "{gpii.deviceReporter}.options.installedSolutionsUrl"
            }
        }
    }, {
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "userLogin",
        "demandSpec": {
            "options": {
                "preInitFunction": "gpii.requests.request.handler.userLoginPreInit",
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.userLogin",
                        "args": ["{request}.req.params.token", "{gpii.requests.request.handler}"]
                    }
                },
                "components": {
                    "requestProxy": "{requestProxy}",
                    "preferencesDataSource": "{flowManager}.preferencesDataSource",
                    "deviceReporterDataSource": "{flowManager}.deviceReporterDataSource",
                    "matchMakerDataSource": "{flowManager}.matchMakerDataSource",
                    "lifecycleManager": "{flowManager}.lifecycleManager"
                },
                "events": {
                    "onRequestEnd": "{request}.events.onRequestEnd",
                    "onUserListener": null,
                    "onPreferences": null,
                    "onDevice": null,
                    "onReadyToMatch": {
                        "events": {
                           "preferences": "onPreferences",
                           "device": "onDevice"
                        },
                        "args": ["{arguments}.preferences.0", "{arguments}.device.0"]
                    },
                    "onMatch": null
                },
                "listeners": {
                    "onUserListener": [{
                        "listener": "{gpii.requests.request.handler}.getPreferences"
                    }, {
                        "listener": "{gpii.requests.request.handler}.getDevice"
                    }],
                    "onReadyToMatch": "{gpii.requests.request.handler}.onReadyToMatchHandler",
                    "onMatch": "{gpii.requests.request.handler}.onMatchHandler",
                    "onRequestEnd": {
                        "listener": "gpii.clearRequest",
                        "args": "{that}"
                    }
                }
            }
        }
    }, {
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "userLogout",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.userLogout",
                        "args": ["{requestProxy}", "{request}.req.params.token", "{flowManager}.lifecycleManager"]
                    }
                }
            }
        }
    }, {
        "demandingName": "rawPreferencesDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL",
            "options": {
                "url": "{gpii.flowManager}.options.urls.preferences",
                "termMap": {
                    "token": "%token"
                }
            }
        }
    }, {
        "demandingName": "preferencesDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawPreferencesDataSource}"
                }
            }
        }
    }, {
        "demandingName": "rawDeviceReporterDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL",
            "options": {
                "url": "{gpii.flowManager}.options.urls.deviceReporter"
            }
        }
    }, {
        "demandingName": "deviceReporterDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawDeviceReporterDataSource}"
                }
            }
        }
    }, {
        "demandingName": "rawMatchMakerDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL",
            "options": {
                "url": "{gpii.flowManager}.options.urls.matchMaker",
                "writable": true
            }
        }
    }, {
        "demandingName": "matchMakerDataSource",
        "contextNames": "gpii.flowManager",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawMatchMakerDataSource}"
                }
            }
        }
    }, {
        "demandingName": "gpii.matchMaker.match",
        "contextNames": "gpii.matchMaker",
        "demandSpec": {
            "funcName": "gpii.matchMaker.match",
            "args": ["{arguments}.0", "{arguments}.1", "{arguments}.2"]
        }
    }, {
        "demandingName": "gpii.matchMaker.prefilterSolutions",
        "contextNames": "gpii.matchMaker",
        "demandSpec": {
            "funcName": "gpii.matchMaker.prefilterSolutions",
            "args": ["{arguments}.0", "{arguments}.1"]
        }
    }, {
        "demandingName": "gpii.matchMaker.filterSolutionsByVersion",
        "contextNames": "gpii.matchMaker",
        "demandSpec": {
            "funcName": "gpii.matchMaker.filterSolutionsByVersion",
            "args": ["{arguments}.0", "{arguments}.1", "{arguments}.2"]
        }
    }, {
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "matchPost",
        "demandSpec": {
            "options": {
                "components": {
                    "matchMaker": "{gpii.matchMaker}"
                },
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.matchPost",
                        "args": ["{that}", "{request}.req.body"]
                    }
                },
                "events": {
                    "onMatchRequest": null,
                    "onRequestEnd": "{request}.events.onRequestEnd",
                    "onSolutionsReporter": null,
                    "onOntologyServer": null,
                    "onReadyToMatch": {
                        "events": {
                           "solutions": "onSolutionsReporter",
                           "ontology": "onOntologyServer"
                        },
                        "args": ["{arguments}.solutions.0", "{arguments}.ontology.0"]
                    }
                },
                "listeners": {
                    "onMatchRequest": [{
                        "listener": "{that}.getSolutions"
                    }, {
                        "listener": "{that}.transformPreferences"
                    }],
                    "onReadyToMatch": "{that}.match",
                    "onRequestEnd": {
                        "listener": "gpii.clearRequest",
                        "args": "{that}"
                    }
                },
                "preInitFunction": "gpii.requests.request.handler.matchPostPreInit"
            }
        }
    }, {
        "demandingName": "solutionsReporter",
        "contextNames": "gpii.matchMaker",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawSolutionsReporter}"
                }
            }
        }
    }, {
        "demandingName": "rawSolutionsReporter",
        "contextNames": "gpii.matchMaker",
        "demandSpec": {
            "funcName": "gpii.dataSource.URL",
            "options": {
                "url": "{gpii.matchMaker}.options.solutionsReporterUrl",
                "termMap": {
                    "os": "%os",
                    "version": "%version"
                }
            }
        }
    }, {
        "demandingName": "ontologyServer",
        "contextNames": "gpii.matchMaker",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawOntologyServer}"
                }
            }
        }
    }, {
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "ontologyConvertPost",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.ontologyConvertPost",
                        "args": ["{that}", "{requestProxy}", "{request}.req.params.to", "{request}.req.body.preferences", "{ontologyServer}.ontologySource"]
                    }
                }
            }
        }
    }, {
        "demandingName": "ontologySource",
        "contextNames": "gpii.ontologyServer",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawOntologySource}"
                }
            }
        }
    }, {
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "userGet",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.userGet",
                        "args": ["{requestProxy}", "{request}.req.params.token", "{preferencesServer}.userSource"]
                    }
                }
            }
        }
    }, {
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "userPost",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.userPost",
                        "args": ["{requestProxy}", "{request}.req", "{preferencesServer}.userSource"]
                    }
                }
            }
        }
    }, {
        "demandingName": "userSource",
        "contextNames": "gpii.preferencesServer",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawUserSource}"
                }
            }
        }
    }, {
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "solutionGet",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.solutionGet",
                        "args": ["{request}.req.params.id", "{requestProxy}.", "{solutionsRegistry}.solutionSource"]
                    }
                }
            }
        }
    }, {
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "solutionPost",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.solutionPost",
                        "args": ["{request}.req", "{requestProxy}", "{solutionsRegistry}.solutionSource"]
                    }
                }
            }
        }
    }, {
        "demandingName": "gpii.requests.request.handler",
        "contextNames": "solutionQuery",
        "demandSpec": {
            "options": {
                "invokers": {
                    "handle": {
                        "funcName": "gpii.requests.request.handler.solutionQuery",
                        "args": ["{requestProxy}", "{request}.req.query.os", "{solutionsRegistry}.solutionQuerySource"]
                    }
                }
            }
        }
    }, {
        "demandingName": "solutionSource",
        "contextNames": "gpii.solutionsRegistry",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawSolutionSource}"
                }
            }
        }
    }, {
        "demandingName": "solutionQuerySource",
        "contextNames": "gpii.solutionsRegistry",
        "demandSpec": {
            "options": {
                "components": {
                    "rawSource": "{rawSolutionQuerySource}"
                }
            }
        }
    }]
}